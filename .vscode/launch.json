            "args": ["--fitting-subj", "0",
                    "--tr", "2000",
                    "--g", "1.01",
                    "--tmax", "300",
                    "--nsubj", "100",
                    "--use-mp",
                    "--data-path", "${workspaceFolder}/Datos/Datasets/DataHCP80", 
                    "--nproc", "32",
                    "--out-path", "${workspaceFolder}/Datos/Results/Results_cluster/test",
                    "--observables", "FC", "swFCD", "phFCD"]
        },
