    "version": "0.2.0",
    "configurations": [
        {
            "name": "Subject 0",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Cluster/neuronumba/prepro_neuronumba.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "/home/cluster/imartin/neuronumba/src:${env:PYTHONPATH}",
                "NUMBA_CACHE_DIR": "/tmp/nn_cache"
